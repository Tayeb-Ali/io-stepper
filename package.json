{
  "name": "io-stepper",
  "version": "1.0.0",
  "description": "steppers components for new G Ionic",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
